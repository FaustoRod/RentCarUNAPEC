<div class="modal fade" id="modelDeleteModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Eliminar Modelo de Vehiculo</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input type="hidden" id="modelId" />
                <div class="container">
                    <dl class="row">
                        <dt class="col-md-4">Marca</dt>
                        <dd class="col-md-8" id="manufacturer"></dd>

                        <dt class="col-md-4">Descripcion</dt>
                        <dd class="col-md-8" id="descriptionName"></dd>
                    </dl>
                    @*@<div class="row">
                            <h4 id="descriptionName"></h4>
                            <input type="hidden" id="modelId"/>
                        </div>*@
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="deleteModel()">Eliminar</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>