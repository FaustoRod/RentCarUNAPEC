@model RentCarUnapec.Core.Models.Vehicle

    <div class="modal fade" id="vehicleSearchModal" tabindex="-1" role="dialog" aria-labelledby="vehicleSearchModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="vehicleSearchModalLabel">Vehiculos</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <form>
                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label asp-for="Model.ManufacturerId" class="control-label"></label>
                                        <select class="form-control" asp-for="Model.ManufacturerId" id="vehicleSearchManufacturerId" onchange="getModels()"></select>
                                        @*<select asp-for="CarRentInformation.VehicleId" class ="form-control" asp-items="Model.VehicleList"></select>*@
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label asp-for="Model.Id" class="control-label"></label>
                                        <select class="form-control" asp-for="Model.Id" id="vehicleSearchModelId" onchange="getVehicles()"></select>
                                        @*<select asp-for="CarRentInformation.VehicleId" class ="form-control" asp-items="Model.VehicleList"></select>*@
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <table class="table">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.PlateNumber)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Model.ManufacturerId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ModelId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.VehicleTypeId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Color)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody id="vehicleSearchBody">
                        </tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>