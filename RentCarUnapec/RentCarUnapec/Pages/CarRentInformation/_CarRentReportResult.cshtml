@model List<RentCarUnapec.Core.Models.CarRentInformation>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].RentSequence)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].VehicleId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].ClientId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].RentDay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].ReturnDay)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RentSequence.Substring(0,4))
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vehicle.VehicleFullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Client.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RentDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReturnDay)
            </td>
        </tr>
}
    </tbody>
</table>
